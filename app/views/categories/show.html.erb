<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th><%= @category.name %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category.albums.each do |album| %>
      <tr>
        <td><%= album.title %></td>
        <td><%= album.url %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
